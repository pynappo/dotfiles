timeout 180 'swaylock --screenshots --clock --indicator --grace 60 --fade-in --effect-blur 5x2'
timeout 300 'hyprctl dispatch dpms off'
timeout 360 'systemctl suspend'
after-resume 'hyprctl dispatch dpms on'
before-sleep 'swaylock --screenshots --clock --indicator --grace 60 --fade-in --effect-blur 5x2'

{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "p:gray",
          "foreground": "p:white",
          "leading_diamond": "\ue0b6",
          "properties": {
            "style": "unique"
          },
          "style": "diamond",
          "template": "\uf74a {{ path .Path .Location }}",
          "trailing_diamond": "\ue0b0",
          "type": "path"
        },
        {
          "background": "p:cyan",
          "background_templates": [
            "{{ if gt .Ahead 0 }}p:green{{ end }}",
            "{{ if gt .Behind 0 }}p:red{{ end }}"
          ],
          "foreground": "p:black",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "powerline",
          "template": " {{ .UpstreamIcon }}{{.Upstream}}{{ .HEAD }}{{ .BranchStatus }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Staging.Changed) (.Working.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0}} \uf692 {{ .StashCount }}{{ end }}{{ if gt .WorktreeCount 0}} \uf1bb {{ .WorktreeCount }}{{ end }} ",
          "type": "git"
        },
        {
          "background": "p:yellow",
          "foreground": "p:black",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \uf0e7 ",
          "type": "root"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background": "p:yellow",
          "background_templates": [
            "{{if eq \"Charging\" .State.String}}p:blue{{end}}",
            "{{if eq \"Discharging\" .State.String}}p:red{{end}}",
            "{{if eq \"Full\" .State.String}}p:green{{end}}"
          ],
          "foreground": "p:black",
          "leading_diamond": "\ue0b6",
          "properties": {
            "charged_icon": "ðŸ”‹",
            "charging_icon": "\ue234",
            "discharging_icon": "\u2301"
          },
          "style": "diamond",
          "template": " {{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }}{{ .Error }} ",
          "trailing_diamond": "\ue0b4",
          "type": "battery"
        },
        {
          "foreground": "p:white",
          "style": "plain",
          "template": " {{ .CurrentDate | date .Format }} ",
          "type": "time"
        }
      ],
      "type": "rprompt"
    }
  ],
  "console_title_template": "{{.UserName}} @ {{.Folder}} ({{.Shell}}{{if .Root}} [Admin]{{end}}) ",
  "final_space": true,
  "osc99": true,
  "palette": {
    "black": "#000000",
    "blue": "#4560f9",
    "cyan": "#72c2ea",
    "gray": "#666666",
    "green": "#76ef76",
    "orange": "#e7b165",
    "pink": "#eaa1ea",
    "purple": "#ab9aed",
    "white": "#ffffff",
    "yellow": "#dcdc42"
  },
  "version": 2
}

{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "console_title_template": "{{.UserName}} @ {{.Folder}} ({{.Shell}}{{if .Root}} [Admin]{{end}}) ",
  "palette": {
    "white": "#ffffff",
    "gray": "#666666",
    "yellow": "#dcdc42",
    "green": "#76ef76",
    "orange": "#e7b165",
    "purple": "#ab9aed",
    "pink": "#eaa1ea",
    "cyan": "#72c2ea",
    "blue": "#4560f9",
    "black": "#000000"
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "type": "path",
          "foreground": "p:white",
          "properties": {
            "style": "unique"
          },
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "î‚°",
          "background": "p:gray",
          "template": "\uf74a {{ path .Path .Location }}"
        },
        {
          "type": "os",
          "style": "powerline",
          "powerline_symbol": "î‚°",
          "foreground": "p:gray",
          "background": "p:white",
          "template": "{{if eq (and (eq .Shell \"pwsh\") (eq .Icon \"\uE62a\")) (and (eq .Shell \"fish\") (eq .Icon \"\uF303\"))}} {{.Shell}} {{.Icon}} {{end}}"
        }, 
        {
          "type":"git",
          "style": "powerline",
          "powerline_symbol": "î‚°",
          "foreground": "p:black",
          "background": "p:cyan",
          "template": " {{.HEAD}}{{.BranchStatus}}{{if .Working.Changed}}({{.Working.String}}{{if .Staging.Changed}} | {{.Staging.String}}){{end}}{{end}}{{ if ne .Upstream \"\" }} â†’ {{.UpstreamIcon}}{{ url .Upstream .UpstreamURL }}{{end}} "
        },
        {
          "type": "root",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "foreground": "p:black",
          "background": "p:yellow",
          "template": " \uF0E7 "
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "type": "rprompt",
      "segments": [
        {
          "type": "battery",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "foreground": "p:black",
          "background": "p:yellow",
          "background_templates": [
            "{{if eq \"Charging\" .State.String}}p:blue{{end}}",
            "{{if eq \"Discharging\" .State.String}}p:red{{end}}",
            "{{if eq \"Full\" .State.String}}p:green{{end}}"
          ],
          "template": "{{ if not .Error }}{{ .Icon }}{{if ne \"Full\" .State.String}}{{.Percentage}}%{{end}}{{end}}",
          "properties": {
            "discharging_icon": "\u2301",
            "charging_icon": "\uE234",
            "charged_icon": "ðŸ”‹"
          }
        },
        {
          "type": "time",
          "style": "plain",
          "foreground": "p:white",
          "template": " [{{ .CurrentDate | date .Format }}]"
        }
      ]
    }
  ],
  "final_space": true,
  "osc99": true,
  "version": 2
}

{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "console_title_template": "{{.UserName}} @ {{.Folder}}({{.Shell}}{{if .Root}} [Admin]{{end}}) ",
  "palette": {
    "white": "#ffffff",
    "gray": "#cccccc",
    "yellow": "#dcdc42",
    "green": "#76ef76",
    "orange": "#e7b165",
    "purple": "#ab9aed",
    "pink": "#eaa1ea",
    "cyan": "#72c2ea",
    "blue": "#4560f9"
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "type": "os",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0d1",
          "foreground": "p:white",
          "background": "p:blue",
          "template": "{{ if .WSL }}WSL at {{ end }}{{.Icon}}"
        },
        {
          "type": "shell",
          "style": "plain",
          "foreground": "#ffffff",
          "background": "#0077c2",
          "background_templates": [
            "{{if eq \"fish\" .Name}}p:orange{{end}}",
            "{{if eq \"pwsh\" .Name}}p:blue{{end}}"
          ],
          "properties": {
            "mapped_shell_names": {
              "pwsh": "🐢"
            }
          }
        },
        {
          "type": "path",
          "foreground": "p:white",
          "properties": {
            "style": "unique"
          },
          "style": "plain",
          "template": " \uf74a {{ path .Path .Location }}"
        },
        {
          "type": "text",
          "foreground": "p:white",
          "style": "plain",
          "template": " >"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "type": "rprompt",
      "segments": [
        {
          "type": "battery",
          "style": "diamond",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "\ue0b0",
          "foreground": "#193549",
          "background": "#ffeb3b",
          "background_templates": [
            "{{if eq \"Charging\" .State.String}}#40c4ff{{end}}",
            "{{if eq \"Discharging\" .State.String}}#ff5722{{end}}",
            "{{if eq \"Full\" .State.String}}#4caf50{{end}}"
          ],
          "template": "{{ if not .Error }}{{ .Icon }}{{if ne \"Full\" .State.String}}{{.Percentage}}%{{end}}{{end}}",
          "properties": {
            "discharging_icon": "\u2301",
            "charging_icon": "\uE234",
            "charged_icon": "🔋"
          }
        },
        {
          "type": "ytm",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "foreground": "#ffffff",
          "background": "#FF0000",
          "properties": {
            "playing_icon": "\uE602 ",
            "paused_icon": "\uF8E3 ",
            "stopped_icon": "\uF04D "
          }
        },
        {
          "type": "time",
          "style": "plain",
          "foreground": "p:white",
          "template": " {{ .CurrentDate | date .Format }}"
        }
      ]
    }
  ],
  "final_space": true,
  "osc99": true,
  "version": 2
}
